[Unit]
Description=Snom han-app client for DSPG cmbs
Requires=hancmbs.service
After=hancmbs.service
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=on-failure
RestartSec=1
ExecStart=/usr/bin/python3 /usr/local/gateway/dectuledspg/base/ule-hub/snom_han_app.py

[Install]
WantedBy=multi-user.target


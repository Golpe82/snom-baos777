[Unit]
Description=Service for getting syslog ambientlightsensor values and temperature values from a Snom device
After=network-online.target
Wants=network-online.target
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=on-failure
RestartSec=1
Environment=PYTHONPATH=/usr/local/snom_baos_777/:
WorkingDirectory=/usr/local/snom_baos_777/
ExecStart=/usr/bin/python3 snomsyslogknx/main.py

[Install]
WantedBy=multi-user.target

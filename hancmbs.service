[Unit]
Description=Starts DECT ULE DSPGs cmbs server for HAN-FUN
Wants=network.target
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=on-failure
RestartSec=1
ExecStart=/usr/local/gateway/dectuledspg/base/tools/cmbs_tcx -usb -han -nomenu

[Install]
WantedBy=multi-user.target

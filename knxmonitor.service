[Unit]
Description=Service for reading from KNX bus and savin status
After=knxurlgateway.service
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=on-failure
RestartSec=1
User=pi
Group=pi
WorkingDirectory=/usr/local/gateway/
ExecStart=/usr/bin/python3 knxmonitor/main.py

[Install]
WantedBy=multi-user.target

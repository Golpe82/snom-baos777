[Unit]
Description=Service for Snom Iot Applications (Django)
After=snomiot.service
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=on-failure
RestartSec=1
User=pi
Group=pi
WorkingDirectory=/usr/local/gateway/iot/
ExecStart=/usr/bin/python3 manage.py runserver 0:8000

[Install]
WantedBy=multi-user.target

[Unit]
Description=Service for Snom Iot Applications (Django)
Requires=network.target
After=snomiot.service
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=on-failure
RestartSec=1
ExecStart=/usr/bin/python3 /usr/local/gateway/iot/manage.py runserver 0:8000

[Install]
WantedBy=multi-user.target
